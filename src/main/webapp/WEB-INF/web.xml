<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"  
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"  
    id="WebApp_ID" version="2.5">  

  <display-name>Java Starter Application</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>PI Application Servlet</servlet-name>
		<servlet-class>com.example.cognitive.personality.DemoServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PI Application Servlet</servlet-name>
		<url-pattern>/pidemo</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>QA Servlet</servlet-name>
		<servlet-class>com.example.cognitive.qa.QAServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>QA Servlet</servlet-name>
		<url-pattern>/QAServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PI Servlet</servlet-name>
		<servlet-class>com.example.cognitive.personality.PIServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PI Servlet</servlet-name>
		<url-pattern>/PIServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Alchemy API</servlet-name>
		<servlet-class>com.example.cognitive.alchemy.AlchemyLanguageServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Alchemy API</servlet-name>
		<url-pattern>/AlchemyAPI</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Alchemy API 2</servlet-name>
		<servlet-class>com.example.cognitive.alchemy.AlchemyLanguageServlet2</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Alchemy API 2</servlet-name>
		<url-pattern>/AlchemyAPI2</url-pattern>
	</servlet-mapping>
</web-app>